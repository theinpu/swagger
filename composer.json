{
	"name" : "theinpu/gossi-swagger",
	"description" : "A php library to manipulate swagger specifications (source - https://github.com/gossi/swagger)",
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "gossi",
			"email" : "github@gos.si"
		}
	],
	"autoload" : {
		"psr-4" : {
			"gossi\\swagger\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"gossi\\swagger\\tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=5.4",
		"phootwork/file" : "~0",
		"phootwork/collection" : "~1",
		"phootwork/json" : "~1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4"
	},
	"keywords" : [
		"swagger",
		"api"
	]
}